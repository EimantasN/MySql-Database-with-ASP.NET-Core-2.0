<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Database</title>

    <environment include="Development">
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body class="padding">

    <ul class="nav nav-tabs" style="padding-bottom:30px">
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="Database">Pagrindinis puslapis</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="Users">Vartotojai</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="items">Skelbimai</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="Categorys">Kategorijos</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="Citys">Miestai</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="Prices">Kainų istorija</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-action="Index" asp-controller="Images">Paveiklėliai</a>
        </li>
    </ul>

    @RenderBody()

    <environment include="Development">
        <script src="~/js/jquery-3.3.1.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/jquery-3.3.1.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>



    @RenderSection("Scripts", required: false)
</body>
</html>
